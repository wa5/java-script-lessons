<!DOCTYPE html>
<html lang="en">
<head>
   <script>
  var per = 55;
   
   if(per >= 80 && per <= 100){
   document.write("you are in merrit.");
   }else if(per >= 60 && per < 80){
   document.write("you are in 1st class.");
   }else if(per >= 45 && per < 60){
   document.write("you are in 2nd class.");
   }else if(per >= 33 && per < 45){
   document.write("you are in 3rd class.");
   }else if(per  < 33){
   document.write("fail.");
   }else{
   document.write("please enter valid percentage.");
   }
  
</script>
   
    <title>Document</title>
</head>
<body>
  
</body>
</html>
